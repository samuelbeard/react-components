{
  "name": "@samuelbeard/react-components",
  "version": "1.0.2",
  "description": "Components for React",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "transpile": "babel src -d dist --copy-files --ignore src/*.scss",
    "prepublishOnly": "npm run transpile",
    "postpublish": "npm run publish-demo",
    "build": "webpack --mode production",
    "deploy": "gh-pages -d storybook-static",
    "publish-demo": "npm run build-storybook && npm run deploy",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelbeard/react-components.git"
  },
  "keywords": [
    "card",
    "react",
    "css"
  ],
  "author": "samuelbeard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samuelbeard/react-components/issues"
  },
  "homepage": "https://github.com/samuelbeard/react-components#readme",
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-info": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^5.1.11",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
